<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title - Hikikomori Shop</title>
    <link rel="icon" href="~/Content/images/favicon.ico">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&amp;display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">

    @Styles.Render("~/Content/style/all.min.css")
    @Styles.Render("~/Content/style/bootstrap.min.css")
    @Styles.Render("~/Content/style/owl.carousel.min.css")
    @Styles.Render("~/Content/style/owl.theme.min.css")
    @Styles.Render("~/Content/style/style.css")
    @Styles.Render("~/Content/style/style.css.map")

    @if (IsSectionDefined("Styled"))
    {@RenderSection("Styles", required: false)}

</head>
<body>
    <header class="header-v4">
        @Html.Partial("_Header")
    </header>

    @RenderBody()


    <div class="cart">
        @Html.Partial("_Footer")
    </div>



    <!--Section for main scripts render-->

    @Scripts.Render("~/Scripts/scr/bootstrap.bundle.min.js")
    @Scripts.Render("~/Scripts/scr/jquery-3.2.1.min.js")
    @Scripts.Render("~/Scripts/scr/jquery-scrolltofixed-min.js")
    @Scripts.Render("~/Scripts/scr/owl.carousel.min.js")
    @Scripts.Render("~/Scripts/scr/popper.min.js")
    @Scripts.Render("~/Scripts/scr/script.js")


    <!--Handler for local scripts-->
    @RenderSection("bootstrap.bundle.min", required: false)
    @RenderSection("jquery-3.2.1.min", required: false)
    @RenderSection("jquery-scrolltofixed-min", required: false)
    @RenderSection("owl.carousel.min", required: false)
    @RenderSection("popper.min", required: false)
    @RenderSection("script", required: false)

</body>
</html>